<Window x:Class="MalevPrototype.CAPTCHAWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MalevPrototype"
        mc:Ignorable="d"
        Title="CAPTCHAWindow" Height="300" Width="500">
    <Grid>
        <StackPanel>
            
            <Canvas x:Name="canvas" Width="250" Height="120" Margin="15">
                
                <Canvas x:Name="canvas1" Margin="220,0,0,0" Width="30" Height="30">


                </Canvas>
                
            </Canvas>
            
            <StackPanel>
                
                <TextBox Name="captchaCode" Width="120" TextChanged="captchaCode_TextChanged" FontSize="20"></TextBox>
                
                <Button Margin="0, 20, 0, 0" FontSize="20" Click="Button_Click" Width="120" Content="Ввести"></Button>
                
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
